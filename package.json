{
	"name": "turboconsolecs",
	"displayName": "turboconsolecs",
	"description": "simply console writeline",
	"version": "0.0.3",
	"publisher": "HendraUswandy",
	"icon": "rocket.png",
	"repository": {
        "type": "git",
        "url": "https://github.com/wijayahend/turboconsolecs"
    },
	"engines": {
		"vscode": "^1.92.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "turboconsolecs.logSelectedCode",
				"title": "Log Selected Code"
			}
		],
		"keybindings": [
			{
				"command": "turboconsolecs.logSelectedCode",
				"key": "ctrl+alt+m",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"type": "object",
			"properties": {
				"turboconsolecs.includeLineNumber": {
					"type": "boolean",
					"default": false,
					"description": "Include filename and line number in Console.WriteLine output."
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.92.0",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"eslint": "^8.57.0",
		"typescript": "^5.4.5",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0"
	}
}
